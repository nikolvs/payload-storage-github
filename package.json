{
  "name": "payload-storage-github",
  "version": "1.0.0",
  "description": "Payload storage adapter for GitHub",
  "author": "Nikolas Silva <nikolas.rsilva@gmail.com>",
  "license": "MIT",
  "keywords": [
    "payload",
    "payloadcms",
    "payload-plugin"
  ],
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:clean": "pnpm clean && pnpm build",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm prepublishOnly",
    "prepublishOnly": "pnpm build:clean"
  },
  "peerDependencies": {
    "payload": "^3.2.2"
  },
  "dependencies": {
    "@payloadcms/plugin-cloud-storage": "^3.2.2",
    "octokit": "^4.0.2"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "^3.0.0",
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.9.2",
    "@types/node": "22.7.5",
    "next": "^15.0.0",
    "payload": "^3.2.2",
    "rimraf": "^6.0.1",
    "typescript": "5.5.3"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    }
  }
}
